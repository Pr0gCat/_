{
  "root": "packages/compiler",
  "sourceRoot": "packages/compiler/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/compiler",
        "main": "packages/compiler/src/index.ts",
        "tsConfig": "packages/compiler/tsconfig.app.json",
        "assets": [
          "packages/compiler/*.md"
        ]
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "compiler:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/compiler/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/compiler"
      ],
      "options": {
        "jestConfig": "packages/compiler/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
